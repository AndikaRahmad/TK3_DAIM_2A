<?php

class UserModel {
    public $id;
    public $name;
    public $role;
    public $email;
    public $password;
    public $phone;
    public $address;

    public function __construct($id, $name, $role, $email, $password, $phone, $address) {
        $this->id = $id;
        $this->name = $name;
        $this->role = $role;
        $this->email = $email;
        $this->password = $password;
        $this->phone = $phone;
        $this->address = $address;
    }
}
